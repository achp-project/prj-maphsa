    <skos:scopeNote xml:lang="en">{"id": "33d0f749-5d92-478a-9f56-229b84e3e677", "value": "Fine-grained metamorphic rock derived from mudstone/shale"}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/f1650ccb-1d8c-4579-9147-920d4d3d5d5e">
    <skos:prefLabel xml:lang="en">{"id": "2106ed87-d9ed-48f0-92d3-1d1e6616fab1", "value": "Metamorphic (undefined)"}</skos:prefLabel>
    <skos:inScheme rdf:resource="http://localhost:8000/d56025c1-40ff-4287-82e3-f5d10638e7c3"/>
    <skos:scopeNote xml:lang="en">{"id": "7f553836-9677-467f-9017-a2a895b545b0", "value": "Rock formed by an intense change in heat and pressure applied to a prexisting igneous or sedimentary rock Use this term where the rock is metamorphic but the specific type (e.g. quartzite, schist) is unknown"}</skos:scopeNote>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/c2510b6e-7a60-40c6-9a1a-bf515ab322c8">
