    <skos:scopeNote xml:lang="en">{"id": "0e03b81b-c52e-4645-a12c-7b9c9c8b8bfe", "value": "Fine- to medium grained  sedimentary rock produced by the accumulation and consolidation of sand-sized clasts"}</skos:scopeNote>
    <skos:prefLabel xml:lang="en">{"id": "2d63848a-fd01-4c75-8606-20cab9358c8e", "value": "Sandstone"}</skos:prefLabel>
  </skos:Concept>
  <skos:Concept rdf:about="http://localhost:8000/3f6c0d9e-734c-4549-b7f6-0483226c0b4f">
